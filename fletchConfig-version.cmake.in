# The full vision-tpl version number.
SET(PACKAGE_VERSION "@vision-tpl_VERSION_MAJOR@.@vision-tpl_VERSION_MINOR@.@vision-tpl_VERSION_PATCH@")

if (NOT "${PACKAGE_FIND_VERSION}" STREQUAL "")
    if (NOT "@vision-tpl_VERSION_MAJOR@" VERSION_EQUAL ${PACKAGE_FIND_VERSION_MAJOR})
        # Major versions represent significant enough differences that we don't
        # want to try to mix them.
        set(PACKAGE_VERSION_UNSUITABLE 1)
    else()
        if("${PACKAGE_VERSION}" VERSION_LESS "${PACKAGE_FIND_VERSION}" )
            set(PACKAGE_VERSION_COMPATIBLE FALSE)
        else()
            set(PACKAGE_VERSION_COMPATIBLE TRUE)
            if( "${PACKAGE_FIND_VERSION}" STREQUAL "${PACKAGE_VERSION}")
                set(PACKAGE_VERSION_EXACT TRUE)
            endif()
        endif()
    endif()
endif()
